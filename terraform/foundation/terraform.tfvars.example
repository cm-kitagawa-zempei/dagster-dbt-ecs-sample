dbt_env_secret_snowflake_private_key = "-----BEGIN PRIVATE KEY-----\nyour_private_key_content_here\n-----END PRIVATE KEY-----"
dbt_env_secret_snowflake_private_key_passphrase = "your_private_key_passphrase"